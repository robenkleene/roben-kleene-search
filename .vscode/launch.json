{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "yarn run start",
      "type": "node",
      "request": "launch",
      "env": {"BROWSER": "none"},
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["run", "start"],
      "serverReadyAction": {
        "pattern": "https?://localhost:([0-9]+)",
        "uriFormat": "http://localhost:%s",
        "action": "openExternally"
      }
    }
  ]
}
